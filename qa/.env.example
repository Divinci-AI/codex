# OpenAI API Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_ORGANIZATION=your-org-id-here
OPENAI_PROJECT=your-project-id-here

# Magentic-One Configuration
MAGENTIC_ONE_WORKSPACE=/workspace/qa
MAGENTIC_ONE_LOG_LEVEL=INFO
MAGENTIC_ONE_MAX_AGENTS=5
MAGENTIC_ONE_TIMEOUT=600

# Agent Configuration
ORCHESTRATOR_MODEL=gpt-4o
ORCHESTRATOR_MAX_TOKENS=4000
ORCHESTRATOR_TEMPERATURE=0.1

# Safety and Security
QA_CONTAINER_ISOLATION=true
QA_NETWORK_RESTRICTIONS=true
QA_FILE_SYSTEM_SANDBOX=true
QA_HUMAN_OVERSIGHT=true
QA_MAX_EXECUTION_TIME=600
QA_MAX_MEMORY_USAGE=2GB
QA_MAX_CPU_USAGE=80

# Testing Configuration
QA_TEST_TIMEOUT=300
QA_PARALLEL_TESTS=4
QA_RETRY_ATTEMPTS=3
QA_REPORT_FORMAT=json,html,markdown

# Logging and Monitoring
QA_LOG_LEVEL=INFO
QA_LOG_FILE=/workspace/qa/logs/qa.log
QA_METRICS_ENABLED=true
QA_METRICS_INTERVAL=30

# Docker Configuration
DOCKER_REGISTRY=localhost:5000
QA_CONTAINER_NAME=codex-qa
QA_NETWORK_NAME=codex-qa-network

# Webhook Testing
WEBHOOK_TEST_PORT=8080
WEBHOOK_TEST_HOST=localhost
WEBHOOK_TIMEOUT=30

# Performance Testing
PERF_BENCHMARK_ITERATIONS=100
PERF_LOAD_TEST_DURATION=300
PERF_MEMORY_THRESHOLD=1GB
PERF_CPU_THRESHOLD=70

# Security Testing
SECURITY_SCAN_ENABLED=true
SECURITY_VULNERABILITY_DB=/workspace/qa/data/vulns.db
SECURITY_REPORT_LEVEL=medium

# Browser Testing (Playwright)
PLAYWRIGHT_BROWSER=chromium
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_TIMEOUT=30000

# Database Testing (Optional)
TEST_DB_URL=sqlite:///workspace/qa/test.db
TEST_DB_POOL_SIZE=5

# Notification Settings (Optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
EMAIL_SMTP_SERVER=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_FROM=qa-bot@yourcompany.com
EMAIL_TO=dev-team@yourcompany.com

# Development Settings
QA_DEBUG=false
QA_VERBOSE=false
QA_DRY_RUN=false
QA_SKIP_CLEANUP=false

# Codex Integration
CODEX_CLI_PATH=/usr/local/bin/codex
CODEX_CONFIG_PATH=/workspace/.codex
CODEX_HOOKS_CONFIG=/workspace/hooks.toml

# Test Data Paths
QA_TEST_DATA_DIR=/workspace/qa/test-data
QA_FIXTURES_DIR=/workspace/qa/fixtures
QA_REPORTS_DIR=/workspace/qa/reports
QA_LOGS_DIR=/workspace/qa/logs

# Agent-Specific Settings
FILE_SURFER_MAX_FILE_SIZE=10MB
FILE_SURFER_ALLOWED_EXTENSIONS=.toml,.json,.yaml,.sh,.py,.js,.ts
WEB_SURFER_ALLOWED_DOMAINS=localhost,127.0.0.1
WEB_SURFER_BLOCKED_DOMAINS=malicious.com,spam.net
CODER_SUPPORTED_LANGUAGES=python,bash,javascript,typescript,rust
TERMINAL_SANDBOX_MODE=true
TERMINAL_ALLOWED_COMMANDS=ls,cat,grep,find,codex

# Resource Limits
MAX_CONCURRENT_AGENTS=5
MAX_AGENT_MEMORY=512MB
MAX_AGENT_CPU=25
MAX_AGENT_EXECUTION_TIME=300

# Backup and Recovery
QA_BACKUP_ENABLED=true
QA_BACKUP_INTERVAL=3600
QA_BACKUP_RETENTION=7d
QA_BACKUP_PATH=/workspace/qa/backups
